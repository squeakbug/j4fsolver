        <node id="T">
            <data key="x">145</data>
            <data key="y">42</data>
        </node>
        <node id="SA">
            <data key="x">135</data>
            <data key="y">30</data>
        </node>
        <node id="NT">
            <data key="x">135</data>
            <data key="y">20</data>
        </node>
        <node id="NSW">
